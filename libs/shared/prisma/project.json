{
  "name": "prisma",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/prisma/src",
  "projectType": "library",
  "tags": ["data", "database", "prisma"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/prisma",
        "main": "libs/shared/prisma/src/index.ts",
        "tsConfig": "libs/shared/prisma/tsconfig.lib.json",
        "assets": ["libs/shared/prisma/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/prisma/jest.config.ts"
      }
    }
  }
}
